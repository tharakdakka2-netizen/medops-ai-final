    "import pandas as pd\n",
    "import joblib\n",
    "from sklearn.ensemble import RandomForestRegressor\n",
    "\n",
    "# Create fresh data\n",
    "df = pd.DataFrame({\n",
    "    'Current_Stock': [100, 50, 200, 30, 10], \n",
    "    'Regional_Flu_Rate': [5, 25, 10, 50, 60], \n",
    "    'Inventory_Required': [110, 200, 150, 350, 450]\n",
    "})\n",
    "\n",
    "# Train and Save\n",
    "model = RandomForestRegressor(n_estimators=100).fit(df[['Current_Stock', 'Regional_Flu_Rate']], df['Inventory_Required'])\n",
    "joblib.dump(model, 'med_inventory_model.pkl')\n",
    "print(\"✅ New model saved! Upload THIS file to GitHub now.\")"
   ]
